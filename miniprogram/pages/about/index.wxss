.logo-container {
  margin-top: 180rpx;
  width: 100%;
  height: 200rpx;
  text-align: center;
}

.logo {
  width: 200rpx;
  height: 200rpx;
  border: 1px solid #f6f6f6;
  display: inline-block;
  background: #f6f6f6;
  border-radius: 50%;
}


.logo image {
  width: 140rpx;
  height: 140rpx;
  margin-top: 40rpx;
  border-radius: 50%;
}

.app-name {
  display: block;
  margin-top: 80rpx;
  width: 100%;
  text-align: center;
}
.app-name .title {
  font-size: 36rpx;
  color: #262626;
}

.app-name .desc {
  margin-top: 20rpx;
  font-size: 28rpx;
  color: #616161;
}
